<h1>Edit restaurant</h1>

<%= form_with(model: @restaurant) do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :address %>
  <%= form.text_field :address %>
  <%= form.label :rating %>
  <%= form.number_field :rating, max: 5, min: 0 %>
  <%= form.submit %>
<% end %>
